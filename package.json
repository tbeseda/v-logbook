{
  "name": "v-logbook",
  "version": "0.1.0",
  "description": "A log viewer for custom entries. A logbook.",
  "author": {
    "name": "Taylor Beseda",
    "email": "tbeseda@gmail.com"
  },
  "repository": "https://github.com/tbeseda/v-logbook",
